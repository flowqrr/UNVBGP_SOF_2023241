@model HashSet<Appointment>

<h3>Your Appointments</h3>

@if (Model != null && Model.Any())
{
    <table>
        <thead>
            <tr>
                <th>Appointment Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var appointment in Model)
            {
                <tr>
                    <td>@appointment.Date</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>You don't have anything scheduled yet.</p>
}
